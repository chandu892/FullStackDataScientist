    "import random\n",
    "import string\n",
    "def pass_gen(length):\n",
    "    chars=string.ascii_letters + string.digits + string.punctuation\n",
    "    passw=\"\".join(random.choice(chars) for _ in range(length))\n",
    "    return passw\n",
    "length=int(input(\"Enter the length: \"))\n",
    "pass_gen(length)"
