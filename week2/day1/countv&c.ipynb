    "def vc(s):\n",
    "    v=['a','e','i','o','u']\n",
    "    vc=0\n",
    "    cc=0\n",
    "    for i in s:\n",
    "        if i==' ':\n",
    "            continue\n",
    "        if i in v:\n",
    "            vc+=1\n",
    "        else:\n",
    "            cc+=1\n",
    "    print(vc)\n",
    "    print(cc)\n",
    "s=input()\n",
    "s.lower()\n",
    "vc(s)"
   ]
