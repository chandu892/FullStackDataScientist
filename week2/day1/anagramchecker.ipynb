    "def is_anagram(n, x):\n",
    "    return sorted(n) == sorted(x)\n",
    "\n",
    "n, x = input().split()\n",
    "if is_anagram(n, x):\n",
    "    print(\"True\")\n",
    "else:\n",
    "    print(\"False\")\n"
